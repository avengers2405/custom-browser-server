generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Entity {
  id String @id @default(cuid()) @map("_id")
  password String @default("pict123")
  role Role
}

model Settings {
  id String @id @default(cuid()) @map("_id")
  type Type @default(BLACKLIST)
  data Json
}

enum Role {
  CLIENT
  MASTER
}

enum Type {
  BLACKLIST
  WHITELIST
}